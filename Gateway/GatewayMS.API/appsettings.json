{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "users-route": {
        "ClusterId": "users-cluster",
        "Match": {
          "Path": "/users/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/users/{**catch-all}"
          }
        ]
      },
      "events-route": {
        "ClusterId": "events-cluster",
        "Match": {
          "Path": "/events/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/events/{**catch-all}"
          }
        ]
      },
      "booking-route": {
        "ClusterId": "booking-cluster",
        "Match": {
          "Path": "/booking/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/booking/{**catch-all}"
          }
        ]
      },
      "seating-route": {
        "ClusterId": "seating-cluster",
        "Match": {
          "Path": "/seating/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/seating/{**catch-all}"
          }
        ]
      },
      "payments-route": {
        "ClusterId": "payments-cluster",
        "Match": {
          "Path": "/payments/{**catch-all}"
        }
      },
      "notifications-route": {
        "ClusterId": "notifications-cluster",
        "Match": {
          "Path": "/hubs/notifications/{**catch-all}"
        }
      },
      "reports-route": {
        "ClusterId": "reports-cluster",
        "Match": {
          "Path": "/reports/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "users-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5038"
          }
        }
      },
      "events-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5040"
          }
        }
      },
      "booking-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5042"
          }
        }
      },
      "seating-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5044"
          }
        }
      },
      "notifications-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5046"
          }
        }
      },
      "payments-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5048"
          }
        }
      },
      "reports-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5050"
          }
        }
      }
    }
  }
}